{
    "name": "xml-world/api-client",
    "description": "Simple connection to the xml.world API",
    "version": "0.1.0",
    "type": "library",
    "license": "Apache-2.0",
    "keywords": [
        "api", "https", "client", "xml"
    ],
    "autoload": {
        "psr-4": {
            "XmlWorld\\ApiClient\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "7.7.0",
        "ext-simplexml": "*",
        "ext-dom": "*"
    },
    "require-dev" : {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^1.9"
    }
}
