{
    "name": "xmlworld/api-client",
    "description": "Simple connection to the xml.world API",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "api", "https", "client", "xml"
    ],
    "homepage": "https://xml.world",
    "support": {
        "docs": "https://xml.world/for-developers",
        "email": "dev@experienceengine.com"
    },
    "autoload": {
        "psr-4": {
            "XMLWorld\\ApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "XMLWorld\\ApiClient\\Test\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.7",
        "ext-simplexml": "*",
        "ext-dom": "*"
    },
    "require-dev" : {
        "phpunit/phpunit": "^9",
        "phpstan/phpstan": "^1.9"
    }
}
